		"target": "NSIS",
		"nsis": {
			"allowToChangeInstallationDirectory": true,
			"oneClick": false,
			"deleteAppDataOnUninstall": true
		},
		"win": {
			"icon": "./icon.ico"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "TestDevelopeer",
